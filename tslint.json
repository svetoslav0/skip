{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "allowJs": true,
    "jsRules": {
        "no-empty": true
    },
    "rules": {
        "variable-name": [
            true,
            "require-const-for-all-caps",
            "allow-leading-underscore",
            "allow-pascal-case",
            "allow-snake-case"
        ],
        "no-console": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "member-ordering": false
    },
    "rulesDirectory": []
}
